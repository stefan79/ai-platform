{
  "name": "client",
  "root": "apps/client",
  "sourceRoot": "apps/client/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/client"],
      "options": {
        "command": "pnpm exec vite build --config apps/client/vite.config.mts"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec vite dev --config apps/client/vite.config.mts --host"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec eslint \"apps/client/src/**/*.{ts,tsx}\""
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec vitest run --config apps/client/vite.config.mts"
      }
    }
  }
}
